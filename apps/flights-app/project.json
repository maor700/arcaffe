{
  "root": "apps/flights-app",
  "sourceRoot": "apps/flights-app/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/web:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "compiler": "babel",
        "outputPath": "dist/apps/flights-app",
        "index": "apps/flights-app/src/index.html",
        "baseHref": "/",
        "main": "apps/flights-app/src/main.tsx",
        "polyfills": "apps/flights-app/src/polyfills.ts",
        "tsConfig": "apps/flights-app/tsconfig.app.json",
        "assets": [
          "apps/flights-app/src/favicon.ico",
          "apps/flights-app/src/assets"
        ],
        "styles": ["apps/flights-app/src/styles.less"],
        "scripts": [],
        "webpackConfig": "@nrwl/react/plugins/webpack"
      },
      "configurations": {
        "production": {
          "fileReplacements": [
            {
              "replace": "apps/flights-app/src/environments/environment.ts",
              "with": "apps/flights-app/src/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false
        }
      }
    },
    "serve": {
      "executor": "@nrwl/web:dev-server",
      "options": {
        "buildTarget": "flights-app:build",
        "hmr": true
      },
      "configurations": {
        "production": {
          "buildTarget": "flights-app:build:production",
          "hmr": false
        },
        "development": {
          "browserTarget": "flights-app:build:development",
          "port": 3006
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/flights-app/**/*.{ts,tsx,js,jsx}"]
      }
    }
  },
  "tags": []
}
